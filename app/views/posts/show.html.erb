<h1><%= @post.title %></h1>
<h3><%= @post.body %></h3>
<h5>Posted at: <%= @post.created_at.strftime('%b %d %y') %></h5>
<%= link_to "Edit Post" , edit_post_path %> | <%= link_to 'Delete Post', @post, :confirm => 'Are you sure to delete this post?', :method => :delete %> | <%= link_to 'Show all post', posts_path %>
